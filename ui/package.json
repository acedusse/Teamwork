{
  "name": "taskmaster-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build && echo 'Bundle analysis complete! Open dist/stats.html to view bundle breakdown.'",
    "build:production": "NODE_ENV=production npm run build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "test": "jest",
    "test:unit": "jest --coverage",
    "test:watch": "jest --watch",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run --spec 'cypress/e2e/**/*.cy.js'",
    "test:e2e:headed": "cypress run --headed --spec 'cypress/e2e/**/*.cy.js'",
    "test:e2e:headless": "cypress run --headless --spec 'cypress/e2e/**/*.cy.js'",
    "test:component": "cypress run --component",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:ci": "npm run lint && npm run test:unit && npm run cypress:run",
    "test:performance": "cypress run --spec 'cypress/e2e/01-navigation-and-layout.cy.js' --config video=true",
    "test:a11y": "cypress run --spec 'cypress/e2e/**/*.cy.js' --env accessibility=true",
    "validate-config": "node scripts/validate-config.js",
    "deploy:prepare": "npm run lint && npm run test:all && npm run build:production",
    "deploy:analyze": "npm run build:analyze && echo 'Review bundle analysis before deployment'",
    "health-check": "node scripts/health-check.js"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.835.0",
    "@aws-sdk/credential-providers": "^3.835.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@mui/x-date-pickers": "^8.5.1",
    "@tanstack/react-query": "^5.20.5",
    "axios": "^1.6.7",
    "cytoscape": "^3.32.0",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.13",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "papaparse": "^5.5.3",
    "quill": "^2.0.3",
    "react": "^18.2.0",
    "react-cytoscapejs": "^2.0.0",
    "react-dom": "^18.2.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.22.1",
    "recharts": "^2.12.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.9",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-jest": "^29.7.0",
    "cypress": "^14.4.1",
    "cypress-axe": "^1.6.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "typescript": "^5.8.3",
    "vite": "^5.1.0"
  }
}
